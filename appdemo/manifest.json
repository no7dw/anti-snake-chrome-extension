{
    "name": "anti-snake",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "anti-snake",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "permissions": [
        "webRequest",
        "tabs",
        "http://*/*",
        "https://*/*",
        "webRequestBlocking"
    ]
}
